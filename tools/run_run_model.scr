/raid8/forecast/proj/uswide/tools/run_model.pl -m $MODEL -p $BASIN -f spinup_nearRT -s 2006-01-01 -e $E_DATE -st spinup_nearRT -i /raid8/forecast/proj/uswide/data/$BASIN/state/retro/vic/state_20051231
